<Window x:Class="wykrywanie_otworkow_test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wykrywanie_otworkow_test"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="button" Content="Button" Height="66" Margin="36,78,268,31" Width="132" Click="button_Click" Grid.Column="1" Grid.Row="1" d:LayoutOverrides="VerticalAlignment"/>
        <Image x:Name="image_capture" Margin="1,1,1,1" d:LayoutOverrides="HorizontalAlignment" Grid.Column="1"  Grid.Row="0" />
        <Button x:Name="btn_steram" Content="steram start" Margin="49,78,169,31" Click="btn_stream_Click" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment" Grid.Row="1"/>
        <Image x:Name="image_stream" Margin="0,0,0,0" Grid.ColumnSpan="1"/>

    </Grid>
</Window>
